extends templates/header
block variables 
	-currentPage = "index"
block main
	section.graphs
		div.first 
			details
				summary Построение графика
				form
					table
						tr
							th Значениe по оси OX
							th Результат
						tr
							td
								input(type="radio" name="absc" value="0")
								| Производитель <br>
							td
								input(type="checkbox" name="ord" value="0")
								| Длина мензуры <br>
								input(type="checkbox" name="ord" value="1")
								| Год производства <br>
					input(type="button" value="Построить график")
		.second 
			details
				summary Сортировка
				form
					select(name="sortFirst")
						option(value="0") Название
						option(value="1") Количество ладов
						option(value="2") Цена
						option(value="3") Материал топа
						option(value="4") Цвет
					input(type="button" value="Сортировка 1-ый уровень")
				form
					select(name="sortSecond")
						option(value="0") Название
						option(value="1") Количество ладов
						option(value="2") Цена
						option(value="3") Материал топа
						option(value="4") Цвет
					input(type="button" value="Сортировка 2-ой уровень")
				form
					select(name="sortThird")
						option(value="0") Название
						option(value="1") Количество ладов
						option(value="2") Цена
						option(value="3") Материал топа
						option(value="4") Цвет
					input(type="button" value="Сортировка 3-ий уровень")


		.third 
			details
				summary Фильтр
				form
					p 
						label(for="BrandName") Производитель <br>
							input(type="text" name="Brand")
					p 
						label(for="topMat") Модель <br>
							input(type="text" name="topMaterial")
					p
						label(for="fret") Количество ладов <br> 
							input(type="text" name="fretNumPre")
							|(Минимум) <br>
							input(type="text" name="fretNumTo")
							|(Максимум) <br>
						label(for="color") Форма корпуса <br>
							input(type="text" name="typeName")
							|	
					p
						label(for="yFrom") Год производства <br> От
							input(type="text" name="yearPre")
							|<br> До
							input(type="text" name="yearTo")
						br
						input(type="button" value="Поиск")
					p
						label(for="yFrom") Длина мензуры <br> От 
							input(type="text" name="menPre")
							|<br> До
							input(type="text" name="menTo")
						br
						input(type="button" value="Поиск" id='menz-search')
					p Количество струн <br>
						input(type="radio" name="stringsNum" value="6")
						|6<br>
						input(type="radio" name="stringsNum" value="7")
						|7<br>
						input(type="radio" name="stringsNum" value="8")
						|8<br>
					input(type="button" value="Поиск" id="string-button")
					input(type="button" value="Refrash" id="refrash-table")

